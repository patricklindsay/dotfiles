alias less="less -r -f"

alias cgs="clear && git status"
alias gs="git status"
alias gd="git diff"
alias g="git"
alias c="commit"
alias co="git checkout"
alias be="bundle exec"
alias dj="bundle exec rake jobs:work"
alias pdfs="bundle exec fuji_pdf_server start"
alias ..='cd ..'
alias ...='cd ../../'
